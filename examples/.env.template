# authlete-test SDK Environment Variables
# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to version control

# Security Configuration
# The SDK supports loading security credentials from environment variables
# with the prefix: AUTHLETETEST_
#
# Security environment variables:
AUTHLETETEST_AUTHLETE=your_authlete_here
AUTHLETETEST_BEARER=your_bearer_here

# Debug Configuration
# Enable HTTP request/response logging for debugging
AUTHLETETEST_DEBUG=true
